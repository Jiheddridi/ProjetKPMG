{
  "name": "COBIT 2019 Evaluation Platform",
  "description": "Plateforme d'évaluation COBIT 2019 avec IA intégrée",
  "repository": "https://github.com/Jiheddridi/ProjetKPMG",
  "stack": "heroku-22",
  "formation": {
    "web": {
      "quantity": 1,
      "size": "basic"
    }
  },
  "addons": [
    {
      "plan": "jawsdb:kitefin"
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/php"
    }
  ],
  "env": {
    "APP_KEY": {
      "generator": "secret"
    },
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LOG_CHANNEL": "errorlog"
  }
}
